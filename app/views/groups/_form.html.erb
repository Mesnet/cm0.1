<%= simple_form_for(@group) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :company %>
    <%= f.input :name %>
    <%= f.input :effectif %>
    <%= f.input :cat %>
    <%= f.input :pend_req %>
    <%= f.input :description %>
    <%= f.input :open %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
